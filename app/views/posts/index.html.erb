<h1>This  is  index.html.erb  file!</h1>
<div class="col-md-12">
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Body</th>
        <th>Created_at</th>
      </tr>
    </thead>
    <tbody>
      <%  @posts.each  do  |post|  %>
        <tr>
          <td><%=  post.title  %></td>
          <td><%=  post.body   %></td>
          <td><%=  post.created_at  %></td>
        </tr>
      <% end  %>
    </tbody>
  </table>
  
</div>
